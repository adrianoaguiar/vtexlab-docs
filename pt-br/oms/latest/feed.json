            "description": "Obtém uma lista (estilo feed) com a atualização de status de pedido.",
            "verb": "GET",
            "route": {
                "template": "/api/oms/pvt/feed/orders/status"
            },
            "attributes": {
                "orderId ": {
                        "type": "string", 
                        "description": "Número do pedido",
                        "required": true               
                      },
                "status": {
                        "type": "string", 
                        "description": "Status do pedido",
                        "required": true               
                      },
                "commitToken ": {
                        "type": "string", 
                        "description": "Token para confirmação da mensagem do feed",
                        "required": true               
                      }
            },
            "example": {
