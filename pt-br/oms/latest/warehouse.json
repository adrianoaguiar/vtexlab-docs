                },
                "scheduledDelivery": {
                    "type": "bool",
                    "description": "Realiza entrega agendada",
                    "required": false
                },
                "maxRangeDelivery": {
                    "type": "int",
                    "description": "Prazo máximo em dias para realizar a entrega agendada",
                    "required": false
