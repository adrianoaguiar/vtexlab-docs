        {
            "title": "Salvar uma doca",
            "description": "Cadastra/atualiza uma doca",
            "verb": "POST",
            "route": {
                "template": "/api/logistics/pvt/configuration/docks"
                
            },
            "arguments": {
                "id": {
                    "type": "string",
                    "description": "Identificador único. Não pode ser alterado",
                    "required": true
                },
                "name": {
                    "type": "string",
                    "description": "Nome/descrição da doca",
                    "required": true
                },
                "priority": {
                    "type": "int",
                    "description": "Valor que define a ordem de priorização da doca no cálculo de SLA. Quanto menor o valor, mais prioritária é a doca",
                    "required": false
                },
                "id": {
                    "type": "TimeSpan",
                    "description": "Tempo para manuseio na doca",
                    "required": false
                },
                "timeFakeOverhead": {
                    "type": "TimeSpan",
                    "description": "Tempo utilizado no desempate no cálculo de SLA. Quanto maior o valor, menos prioritária é a doca",
                    "required": false
                },
                "freightTableIds": {
                    "type": "array",
                    "description": "Ids das transportadoras associadas à doca",
                    "required": false
                },
                "salesChannels": {
                    "type": "array",
                    "description": "Canais de venda associados à doca",
                    "required": false
                }
